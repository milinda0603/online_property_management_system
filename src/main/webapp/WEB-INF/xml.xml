<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">


     <servlet>
     	<servlet-name> InserServlet</servlet-name>
     	<servlet-class>paymentpack.InserServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>InserServlet</servlet-name>
     	<url-pattern>/InserServlet</url-pattern>
     </servlet-mapping>


	 <servlet>
     	<servlet-name> PaymentGetAll</servlet-name>
     	<servlet-class>paymentpack.PaymentGetAll</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>PaymentGetAll</servlet-name>
     	<url-pattern>/PaymentGetAll</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>UpdateServlet</servlet-name>
     	<servlet-class>paymentpack.UpdateServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>UpdateServlet</servlet-name>
     	<url-pattern>/UpdateServlet</url-pattern>
     </servlet-mapping>
     
     <servlet>
     	<servlet-name>DeleteServlet</servlet-name>
     	<servlet-class>paymentpack.DeleteServlet</servlet-class>
     </servlet>
     <servlet-mapping>
     	<servlet-name>DeleteServlet</servlet-name>
     	<url-pattern>/DeleteServlet</url-pattern>
     </servlet-mapping>
     

</web-app>